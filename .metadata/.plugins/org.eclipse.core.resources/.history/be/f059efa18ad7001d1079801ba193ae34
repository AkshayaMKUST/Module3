package com.ust.ioc2;

public class Point {
	
	Point p1;
	Point p2;
	
	private int x;
	private int y;
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	
	public void drawline() {
		System.out.println("From "+p1.getX()+","+p1.getY());
		System.out.println("to "+p2.getX()+","+p2.getY());
	}

}
